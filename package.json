{
    "name": "nadundesilva-website",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@emotion/cache": "~11.7.1",
        "@emotion/react": "~11.9.0",
        "@emotion/server": "~11.4.0",
        "@emotion/styled": "~11.8.1",
        "@fortawesome/fontawesome-common-types": "~6.1.1",
        "@fortawesome/fontawesome-svg-core": "~6.1.1",
        "@fortawesome/free-brands-svg-icons": "~6.1.1",
        "@fortawesome/free-solid-svg-icons": "~6.1.1",
        "@fortawesome/react-fontawesome": "~0.1.18",
        "@mui/icons-material": "~5.6.0",
        "@mui/lab": "~5.0.0-alpha.76",
        "@mui/material": "~5.6.0",
        "next": "^12.1.4",
        "next-compose-plugins": "~2.2.1",
        "next-pwa": "~5.4.7",
        "postcss": "~8.4.12",
        "react": "~18.0.0",
        "react-dom": "~18.0.0"
    },
    "scripts": {
        "dev": "PUBLIC_URL=http://localhost:3000 next dev",
        "build": "next build && next export && node generate-sitemap.js",
        "build:analyze": "ANALYZE=true next build",
        "start": "next start",
        "lint": "npm run lint:ts && npm run lint:css",
        "lint:ts": "next lint",
        "lint:css": "stylelint *.css **/*.css",
        "format": "npx prettier --write .",
        "test": "jest --watch",
        "test:ci": "jest --ci",
        "cypress:open": "cypress open",
        "cypress:run": "cypress run --reporter cypress-image-snapshot/reporter",
        "cypress:snapshots:update": "npm run cypress:run -- --env updateSnapshots=true"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/plugin-proposal-do-expressions": "~7.16.7",
        "@cypress/code-coverage": "^3.9.12",
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@next/bundle-analyzer": "^12.1.4",
        "@testing-library/cypress": "^8.0.2",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.0.0",
        "@testing-library/user-event": "~14.0.4",
        "@types/cypress-image-snapshot": "~3.1.6",
        "@types/jest": "^27.4.1",
        "@types/node": "~17.0.23",
        "@types/react": "~17.0.43",
        "@types/react-dom": "~17.0.14",
        "@typescript-eslint/eslint-plugin": "~5.18.0",
        "@typescript-eslint/parser": "~5.18.0",
        "babel-plugin-import": "~1.13.3",
        "cypress": "^9.5.3",
        "cypress-image-snapshot": "~4.0.1",
        "eslint": "~8.12.0",
        "eslint-config-next": "~12.1.4",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-standard-with-typescript": "~21.0.1",
        "eslint-plugin-import": "~2.26.0",
        "eslint-plugin-node": "~11.1.0",
        "eslint-plugin-promise": "~6.0.0",
        "eslint-plugin-react": "~7.29.4",
        "jest": "^27.5.1",
        "next-router-mock": "^0.6.5",
        "nyc": "^15.1.0",
        "prettier": "2.6.2",
        "sitemap": "~7.1.1",
        "stylelint": "^14.6.1",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-standard": "^25.0.0",
        "typescript": "~4.6.3"
    }
